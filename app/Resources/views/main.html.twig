<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title>{% block title %}{% endblock %}</title>
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    {% block stylesheets %}{% endblock %}
</head>
<body id="{% block id '' %}">
    {% block header %}{% endblock %}
    {% block body %}{% endblock %}
    <footer>
        &copy; 2013 - {{ 'now' | date('Y') }} <br>
        Luis González Rodríguez
    </footer>

    {% block javascripts %}
        <script src="{{ asset('bundles/dentoletigeneral/js/jquery-2.1.0.min.js') }}"></script>
        {#
        {% javascripts '@DentoletiPatientBundle/Resources/public/js/ajax-calls.js'%}
            <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}
        #}
        {% javascripts '@DentoletiPatientBundle/Resources/public/js/tabs.js'%}
        <script type="text/javascript" src="{{ asset_url }}"></script>
        {% endjavascripts %}

    {% endblock %}
</body>
</html>